{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "sales",
        "Match": {
          "Path": "Sales/{**catch-all}"
        }
      },
      "route2": {
        "ClusterId": "fin",
        "Match": {
          "Path": "Finance/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "sales": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:5001"
          }
        }
      },
      "fin": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:5002"
          }
        }
      }
    }
  }
}
